config microbe 'webadmin'
        option enable '1'
        option port '85'
        option home '/www'
        option project 'mixnet'
        option password '1234'

config microbe 'pinger'
        option enable '1'
        option priority '3g-wan1,3g-wan2'
        option checked '8.8.8.8'
        option host '192.168.1.1'

